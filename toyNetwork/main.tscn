script/source = "extends VBoxContainer

func _ready():
	get_tree().connect(\"network_peer_connected\", self, \"network_peer_connected\")
	get_tree().connect(\"network_peer_disconnected\", self, \"network_peer_disconnected\")
	get_tree().connect(\"connected_to_server\", self, \"connected_to_server\")
	get_tree().connect(\"connection_failed\", self, \"connection_failed\")
	get_tree().connect(\"server_disconnected\", self, \"server_disconnected\")
	Steam.connect(\"join_requested\",self,\"joinRequested\")

func network_peer_connected(id):
	createLabel(\"network_peer_connected\" + String(id))
	
func network_peer_disconnected(id):
	createLabel(\"network_peer_disconnected\" + String(id))
	
func connected_to_server():
	createLabel(\"connected_to_server\")
	
func connection_failed():
	createLabel(\"connection_failed\")
	
func server_disconnected():
	createLabel(\"server_disconnected\")



func joinRequested(a,b):
	createLabel(\"joinRequested:%d,%d\"%[a,b])


func createLabel(text):
	var asdf = Label.new();
	asdf.text = text
	add_child(asdf)
	yield(get_tree().create_timer(15),\"timeout\")
	asdf.queue_free()

func _process(delta):
	get_parent().scroll_vertical  = 9999999999999

#func _on_VBoxContainer_resized():
"

