[sub_resource type="GDScript" id=5]
script/source = "extends HBoxContainer




func _on_host_pressed():
	clearNetwork()
	var s = SteamNetworkPeer.new()
	s.create_lobby(SteamNetworkPeer.FRIENDS_ONLY,2);
	get_tree().network_peer = s
	var status = yield(s,\"continue\")
	print(status)
	pass

func _on_connect_pressed():
	clearNetwork()
	get_tree().network_peer = SteamNetworkPeer.new()

func clearNetwork():
	var p = get_tree().network_peer
	if p:
		if p.has_method(\"close_connection\"):
			p.close_connection()
		if p.has_method(\"close\"):
			p.close()
	get_tree().network_peer = null


"

