script/source = "extends HBoxContainer

export(NodePath) onready var pingButton = get_node(pingButton) as Control


func _ready():
	Steam.connect(\"join_requested\",self,\"joinRequested\")


func _on_host_pressed():
	pingButton.visible = true
	clearNetwork()
	var s = SteamNetworkPeer.new()
	s.create_server(SteamNetworkPeer.PUBLIC,32);
	get_tree().network_peer = s

func _on_overlay_pressed():
	Steam.activateGameOverlay()

func clearNetwork():
	var p = get_tree().network_peer
	if p:
		if p.has_method(\"close_connection\"):
			p.close_connection()
		if p.has_method(\"close\"):
			p.close()
	get_tree().network_peer = null


func joinRequested(lobbyId,steamId):
	var s = SteamNetworkPeer.new()
	s.create_client(lobbyId);
	get_tree().network_peer = s;
	

"

