[sub_resource type="GDScript" id=6]
script/source = "extends HBoxContainer


const port = 7777

func _on_host_pressed():
	clearNetwork()
	var t = get_tree()
	var p = NetworkedMultiplayerENet.new()
	p.create_server(port)
	t.network_peer = p
	pass

func _on_connect_pressed():
	clearNetwork()
	var t = get_tree()
	var p = NetworkedMultiplayerENet.new()
	p.create_client(\"localhost\",port)
	t.network_peer = p
	pass

func clearNetwork():
	var p = get_tree().network_peer
	if p:
		if p.has_method(\"close_connection\"):
			p.close_connection()
		if p.has_method(\"close\"):
			p.close()
	get_tree().network_peer = null
"

